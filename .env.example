# OpenAI API Key (for gpt-5, gpt-5-mini-2025-08-07, gpt-4o-mini-2024-07-18)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic API Key (for claude-3-5-haiku-20241022)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Judge model configuration
JUDGE_MODEL=gpt-5

# API Pricing Configuration (per million tokens)
# OpenAI Pricing
GPT_5_INPUT_PRICE=1.25
GPT_5_OUTPUT_PRICE=10.0
GPT_5_CACHE_PRICE=0.125
GPT_5_MINI_INPUT_PRICE=0.25
GPT_5_MINI_OUTPUT_PRICE=2.0
GPT_4O_MINI_INPUT_PRICE=0.15
GPT_4O_MINI_OUTPUT_PRICE=0.60

# Anthropic Pricing
CLAUDE_3_5_HAIKU_INPUT_PRICE=0.80
CLAUDE_3_5_HAIKU_OUTPUT_PRICE=4.0

# Optional: Custom model endpoints (if using different providers)
# OPENAI_BASE_URL=https://api.openai.com/v1
# ANTHROPIC_BASE_URL=https://api.anthropic.com

# Optional: Cost monitoring settings
DAILY_BUDGET_USD=50.0
ENABLE_COST_TRACKING=true

# Optional: Cache settings for semantic caching
ENABLE_SEMANTIC_CACHE=true
CACHE_SIMILARITY_THRESHOLD=0.85

# Optional: Concurrency settings
MAX_CONCURRENT_OPENAI_REQUESTS=5
MAX_CONCURRENT_ANTHROPIC_REQUESTS=3

# Optional: Logging settings
LOG_LEVEL=INFO
ENABLE_DETAILED_LOGS=false